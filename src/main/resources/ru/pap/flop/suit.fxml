<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="110.0" prefWidth="30.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.pap.controllers.flop.Suit">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="SOMETIMES"/>
                <RowConstraints vgrow="SOMETIMES"/>
                <RowConstraints vgrow="SOMETIMES"/>
                <RowConstraints vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <fx:include fx:id="suitRow_0" source="card.fxml"/>
                <fx:include fx:id="suitRow_1" source="card.fxml" GridPane.rowIndex="1"/>
                <fx:include fx:id="suitRow_2" source="card.fxml" GridPane.rowIndex="2"/>
                <fx:include fx:id="suitRow_3" source="card.fxml" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
