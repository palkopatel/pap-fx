<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="200.0" prefWidth="520" maxWidth="520.0" fx:controller="ru.pap.controllers.flop.Flop" >
    <!--<padding>-->
        <!--<Insets bottom="0.0" left="4.0" right="4.0" top="2.0"/>-->
    <!--</padding>-->
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0" id="grid-one">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="30" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" maxHeight="90" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="1 - Флоп">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Label>
                <fx:include source="suits_table.fxml" GridPane.rowIndex="1"/>
                <Label text="Board: Kc 7s 5s" GridPane.rowIndex="2" fx:id="boardId"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
